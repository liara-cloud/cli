{
  "name": "@liara/cli",
  "description": "The command line interface for Liara",
  "version": "3.4.0",
  "author": "Mhe <mhemrg120@gmail.com> (http://liara.ir/) @mhemrg",
  "type": "module",
  "bin": {
    "liara": "./bin/run.js"
  },
  "bugs": "https://github.com/liara-ir/liara-cli/issues",
  "dependencies": {
    "@oclif/core": "^2.8.0",
    "@oclif/plugin-autocomplete": "^2.1.8",
    "@oclif/plugin-help": "^5.1.23",
    "@oclif/plugin-version": "^1.1.4",
    "ansi-escapes": "^6.1.0",
    "async-retry": "^1.3.3",
    "bytes": "^3.1.0",
    "chalk": "^5.2.0",
    "email-prompt-ts": "0.3.2",
    "email-validator": "^2.0.4",
    "form-data": "^4.0.0",
    "fs-extra": "^11.1.1",
    "globby": "^13.1.3",
    "got": "^12.6.0",
    "https-proxy-agent": "^5.0.0",
    "ignore": "^5.2.4",
    "inquirer": "9.1.5",
    "moment": "^2.24.0",
    "ora": "^6.3.0",
    "progress": "^2.0.3",
    "semver": "^7.3.5",
    "shamsi-date-converter": "^1.0.5",
    "tar": "^6.1.13",
    "ua-parser-js": "^1.0.35",
    "update-notifier": "^6.0.2",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.5",
    "@types/bytes": "^3.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.0",
    "@types/node": "18.15.11",
    "@types/progress": "^2.0.3",
    "@types/semver": "^7.3.13",
    "@types/tar": "^6.1.4",
    "@types/ua-parser-js": "^0.7.36",
    "@types/update-notifier": "^6.0.2",
    "@types/ws": "^8.5.4",
    "eslint": "^8.37.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "husky": "^8.0.3",
    "oclif": "^3.7.3",
    "prettier": "^2.8.7",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "bin",
    "lib",
    "npm-shrinkwrap.json",
    "oclif.manifest.json"
  ],
  "homepage": "https://github.com/liara-ir/liara-cli",
  "keywords": [
    "liara",
    "liara cli",
    "deploy",
    "deployment",
    "cloud",
    "docker"
  ],
  "license": "MIT",
  "exports": "./lib/index.js",
  "oclif": {
    "bin": "liara",
    "commands": "./lib/commands",
    "topicSeparator": " ",
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ],
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-version"
    ],
    "topics": {
      "app": {
        "description": "display commands for working with apps"
      },
      "db": {
        "description": "display commands for working with databases"
      },
      "account": {
        "description": "display commands for working with accounts"
      },
      "disk": {
        "description": "display commands for working with disks"
      },
      "env": {
        "description": "display commands for configuring app environment variables"
      },
      "plan": {
        "description": "list available plans"
      }
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.spec\\.ts$",
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    }
  },
  "repository": "liara-ir/liara-cli",
  "scripts": {
    "lint": "eslint . --ext .ts --config .eslintrc",
    "format": "prettier \"**/*.ts\" \"**/*.js\" \"**/*.json\" --ignore-path ./.prettierignore --write",
    "postpack": "rm -f oclif.manifest.json tsconfig.tsbuildinfo",
    "prepack": "set -ex; rm -rf lib && tsc -b && oclif manifest && oclif readme",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "version": "oclif readme && git add README.md",
    "readme": "oclif readme",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "types": "./lib/index.d.ts"
}
