{
  "name": "@liara/cli",
  "description": "The command line interface for Liara",
  "version": "3.2.2",
  "author": "Mhe <mhemrg120@gmail.com> (http://liara.ir/) @mhemrg",
  "bin": {
    "liara": "bin/run"
  },
  "bugs": "https://github.com/liara-ir/liara-cli/issues",
  "dependencies": {
    "@oclif/core": "^1.13.10",
    "@oclif/plugin-autocomplete": "^1.3.0",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-version": "^1.1.2",
    "ansi-escapes": "^4.1.0",
    "async-retry": "^1.3.3",
    "bytes": "^3.1.0",
    "chalk": "^2.4.2",
    "email-prompt-ts": "0.3.2",
    "email-validator": "^2.0.4",
    "form-data": "^4.0.0",
    "fs-extra": "^7.0.1",
    "globby": "^11.0.0",
    "got": "^11.8.2",
    "https-proxy-agent": "^5.0.0",
    "ignore": "^5.1.0",
    "inquirer": "8.1.2",
    "minipass": "3.1.5",
    "moment": "^2.24.0",
    "ora": "^5.4.1",
    "progress": "^2.0.3",
    "semver": "^7.3.5",
    "shamsi-date-converter": "^1.0.5",
    "tar": "^6.1.10",
    "tslib": "^1.9.3",
    "ua-parser-js": "^1.0.32",
    "update-notifier": "^5.1.0",
    "ws": "^8.2.3"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.5",
    "@types/bytes": "^3.0.0",
    "@types/concat-stream": "^1.6.0",
    "@types/fs-extra": "^5.0.5",
    "@types/inquirer": "^7.3.3",
    "@types/jest": "^27.4.1",
    "@types/node": "15.14",
    "@types/progress": "^2.0.3",
    "@types/semver": "^7.3.9",
    "@types/tar": "^4.0.5",
    "@types/ua-parser-js": "^0.7.36",
    "@types/update-notifier": "^5.1.0",
    "@types/ws": "^8.2.0",
    "concat-stream": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "husky": "^8.0.0",
    "oclif": "^3",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^27.1.4",
    "ts-node": "^8.0.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "bin",
    "lib",
    "npm-shrinkwrap.json",
    "oclif.manifest.json"
  ],
  "homepage": "https://github.com/liara-ir/liara-cli",
  "keywords": [
    "liara",
    "liara cli",
    "deploy",
    "deployment",
    "cloud",
    "docker"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "liara",
    "commands": "./lib/commands",
    "topicSeparator": " ",
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ],
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-version"
    ],
    "topics": {
      "app": {
        "description": "display commands for working with apps"
      },
      "db": {
        "description": "display commands for working with databases"
      },
      "account": {
        "description": "display commands for working with accounts"
      },
      "disk": {
        "description": "display commands for working with disks"
      },
      "env": {
        "description": "display commands for configuring app environment variables"
      },
      "plan": {
        "description": "list available plans"
      }
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "repository": "liara-ir/liara-cli",
  "scripts": {
    "lint": "eslint . --ext .ts --config .eslintrc",
    "format": "prettier \"**/*.ts\" \"**/*.js\" \"**/*.json\" --ignore-path ./.prettierignore --write",
    "postpack": "rm -f oclif.manifest.json tsconfig.tsbuildinfo",
    "prepack": "set -ex; rm -rf lib && tsc -b && oclif manifest && oclif readme",
    "test": "jest",
    "version": "oclif readme && git add README.md",
    "readme": "oclif readme",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "types": "lib/index.d.ts"
}
