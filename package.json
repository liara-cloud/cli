{
  "name": "@liara/cli",
  "description": "The command line interface for Liara",
  "version": "2.18.0",
  "author": "Mhe <mhemrg120@gmail.com> (http://liara.ir/) @mhemrg",
  "bin": {
    "liara": "bin/run"
  },
  "bugs": "https://github.com/liara-ir/liara-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.12",
    "@oclif/config": "^1.12.12",
    "@oclif/errors": "^1.2.2",
    "@oclif/plugin-autocomplete": "^0.3.0",
    "@oclif/plugin-help": "^2.1.6",
    "ansi-escapes": "^4.1.0",
    "async-retry": "^1.2.3",
    "axios": "^0.21.1",
    "bytes": "^3.1.0",
    "chalk": "^2.4.2",
    "cli-ux": "^5.2.1",
    "email-prompt-ts": "0.3.2",
    "email-validator": "^2.0.4",
    "form-data": "^4.0.0",
    "fs-extra": "^7.0.1",
    "globby": "^11.0.0",
    "got": "^11.8.2",
    "https-proxy-agent": "^5.0.0",
    "ignore": "^5.1.0",
    "inquirer": "8.1.2",
    "moment": "^2.24.0",
    "ora": "^5.4.1",
    "progress": "^2.0.3",
    "shamsi-date-converter": "^1.0.5",
    "tar": "^6.1.10",
    "tslib": "^1.9.3",
    "update-notifier": "^5.1.0",
    "ws": "^8.2.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "@oclif/tslint": "^3.1.1",
    "@types/async-retry": "^1.4.0",
    "@types/bytes": "^3.0.0",
    "@types/chai": "^4.1.7",
    "@types/concat-stream": "^1.6.0",
    "@types/fs-extra": "^5.0.5",
    "@types/inquirer": "^7.3.3",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.12.6",
    "@types/progress": "^2.0.3",
    "@types/request": "^2.48.1",
    "@types/tar": "^4.0.5",
    "@types/update-notifier": "^5.1.0",
    "@types/ws": "^8.2.0",
    "chai": "^4.2.0",
    "concat-stream": "^2.0.0",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "bin",
    "lib",
    "npm-shrinkwrap.json",
    "oclif.manifest.json"
  ],
  "homepage": "https://github.com/liara-ir/liara-cli",
  "keywords": [
    "liara",
    "liara cli",
    "deploy",
    "deployment",
    "cloud",
    "docker"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "liara",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ],
    "topics": {
      "app": {
        "description": "display commands for working with apps"
      },
      "account": {
        "description": "display commands for working with accounts"
      },
      "disk": {
        "description": "display commands for working with disks"
      },
      "env": {
        "description": "display commands for configuring app environment variables"
      },
      "plan": {
        "description": "list available plans"
      }
    }
  },
  "repository": "liara-ir/liara-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json tsconfig.tsbuildinfo",
    "posttest": "tslint -p test -t stylish",
    "prepack": "set -ex; rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "readme": "oclif-dev readme"
  },
  "types": "lib/index.d.ts"
}
